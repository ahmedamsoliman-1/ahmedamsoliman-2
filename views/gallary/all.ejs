<%- include ('../partials/header') %>

<div class="container">
  <header class="jumbotron">
    <div class="container">
      <h1>Weclome To My gallary!</h1>
      <a>View my ALL pics throuh time</a>
      <div><a>Number of image: <%= all.length %></a></div>
    </div>
  </header>

  <div class="row text-center" style="display: flex; flex-wrap: wrap">
    <% all.forEach(function(pic) { %>
    <div class="col-md-3 col-sm-6">
      <div class="thumbnail">
        <img src="<%= pic %>" />
        <div class="caption">
          <h4>
            <%= pic.split("ahmedalisolimanpics/")[1].replace(".jpg", "") %>
          </h4>
        </div>
      </div>
    </div>
    <% }); %>
  </div>
</div>
