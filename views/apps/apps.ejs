<%- include ('../partials/header') %>


<header class="jumbotron">
    <h1>Weclome To The Application Section</h1>
</header>

<div class="container">
    <div class="row text-center" style="display: flex; flex-wrap: wrap;">
        <% apps.forEach(function(app) { %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                    <img class="imgger" sizes="" src="<%= app.image %>">
                    <div class="caption">
                        <h4> <%= app.title %> </h4>
                    </div>
                    <p>
                        <a target="_blank" href="<%= app.link %>" class="btn btn-primary">Open</a>
                    </p>
                </div>
            </div>
        <% }); %>
    </div>
</div>
</div>

