<%- include ('partials/header') %>


<header class="jumbotron">
    <h1>Weclome To The Certifications Section</h1>
</header>

    <header class="jumbotron">
        <h2>AWS Badges</h2>
        <a href="https://www.credly.com/users/ahmed-ali-mohamed-soliman/badges" target="_blank" class="btn btn-warning">Credentials</a>
        <h4>count: <%= aws_badges.length %></h4>
    </header>
    <div class="container">
        <% aws_badges.forEach(function(pic) { %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                    <img src="<%= pic %>">
                    <div class="caption">
                        <!-- https://s3.amazonaws.com/ahmedalimsoliman-aws-certs/aws-badges/AWS Architecting.png -->
                        <h4> <%= pic.split("ahmedalimsoliman-aws-certs/")[1].replace(".png", "").replace("aws-badges/", "") %> </h4>
                    </div>
                </div>
            </div>
        <% }); %>
    </div>
    
