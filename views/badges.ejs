<%- include ('partials/header') %>


<header class="jumbotron">
    <h1>Weclome To The Certifications Section</h1>
</header>

<header class="jumbotron">
    <h2>OA University Certifications</h2>
    <h4>count: <%= oau.length %></h4>
</header>
<div class="container">
        <% oau.forEach(function(pic) { %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                    <img src="<%= pic %>">
                    <div class="caption">
                        <h4> <%= pic.split("ahmedalimsoliman-oau-certs/")[1].replace(".jpg", "") %> </h4>
                    </div>
                    <p>
                        <a href="<%= pic %>" class="btn btn-primary">Open</a>
                    </p>
                </div>
            </div>
        <% }); %>
    </div>
</div>


