<%- include ('partials/header') %>


<header class="jumbotron">
    <h1>Weclome To The Certifications Section</h1>
</header>

<header class="jumbotron">
    <h2>AWS Certifications</h2>
    <a href="https://www.credly.com/users/ahmed-ali-mohamed-soliman/badges" target="_blank" class="btn btn-warning">Credentials</a>
    <h4>count: <%= awscert.length %></h4>
</header>
<div class="container">
        <% awscert.forEach(function(pic) { %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                    <img src="<%= pic %>">
                    <div class="caption">
                        <h4> <%= pic.split("ahmedalimsoliman-aws-certs/")[1].replace(".png", "") %> </h4>
                    </div>
                    <p>
                        <a href="<%= pic %>" target="_blank" class="btn btn-warning">Open</a>
                    </p>
                </div>
            </div>
        <% }); %>
    </div>
</div>

